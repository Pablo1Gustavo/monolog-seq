{
    "name": "pablo1gustavo/monolog-seq",
    "description": "Integrates Monolog with Seq using HTTP ingestion, enabling structured event logging to a centralized Seq server for enhanced log management.",
    "type": "library",
    "require": {
        "monolog/monolog": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pablo1Gustavo\\MonologSeq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pablo1Gustavo\\MonologSeq\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Pablo Gustavo Fernandes Maia",
            "email": "pablo1gustavo.dev@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "test": "@php vendor/bin/phpunit --colors --testdox"
    }
}
